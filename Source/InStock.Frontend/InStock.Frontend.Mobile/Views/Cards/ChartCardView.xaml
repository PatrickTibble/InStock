<?xml version="1.0" encoding="utf-8" ?>
<frames:PrimaryCardFrame 
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:vm="clr-namespace:InStock.Frontend.Core.ViewModels.Cards;assembly=InStock.Frontend.Core"
    x:DataType="vm:ChartViewModel"
    Margin="20"
    xmlns:shadows="clr-namespace:InStock.Frontend.Mobile.Views.Shadows"
    xmlns:frames="clr-namespace:InStock.Frontend.Mobile.Views.Frames"
    x:Class="InStock.Frontend.Mobile.Views.Cards.ChartCardView">
    <Frame.Shadow>
        <shadows:PrimaryShadow />
    </Frame.Shadow>
    <VerticalStackLayout>
        <Label 
            Text="Welcome to .NET MAUI!"
            VerticalOptions="Center" 
            HorizontalOptions="Center" />
    </VerticalStackLayout>
</frames:PrimaryCardFrame>